<view class="rec_tabs">
    <view class="rec_tabs_title">
        <view wx:for="{{tabs}}" wx:key="id" class="title_item {{item.isActive?'active':''}}" bindtap="handleItemTabs" data-index="{{index}}">
            {{item.value}}
        </view>
    </view>
    <view class="rec_tabs_content"></view>
</view>